     "text": [
      "RMSLE function defined.\n"
     ]
    }
   ],
   "source": [
    "import numpy as np\n",
    "from sklearn.metrics import mean_squared_log_error\n",
    "\n",
    "def compute_rmsle(y_test: np.ndarray, y_pred: np.ndarray, precision: int = 2) -> float:\n",
    "    \"\"\"\n",
    "    Compute the Root Mean Squared Logarithmic Error (RMSLE) between\n",
    "    actual and predicted sale prices.\n",
    "\n",
    "    Parameters:\n",
    "    y_test (np.ndarray): True values\n",
    "    y_pred (np.ndarray): Predicted values\n",
    "    precision (int): Number of decimal places to round the RMSLE\n",
    "\n",
    "    Returns:\n",
    "    float: RMSLE value\n",
    "    \"\"\"\n",
    "    # Ensure predictions are non-negative\n",
    "    y_pred = np.maximum(0, y_pred)\n",
    "    rmsle = np.sqrt(mean_squared_log_error(y_test, y_pred))\n",
    "    return round(rmsle, precision)\n",
    "\n",
   ]
  },

    "from sklearn.preprocessing import StandardScaler, OneHotEncoder\n",
    "import numpy as np\n",
    "\n",
    "X_train_num = X_train[continuous_features].copy()\n",
    "X_test_num = X_test[continuous_features].copy()\n",
    "\n",
    "X_train_cat = X_train[categorical_features].copy()\n",
    "X_test_cat = X_test[categorical_features].copy()\n",
    "\n",
    "X_train_num = X_train_num.fillna(X_train_num.mean())\n",
    "X_test_num = X_test_num.fillna(X_train_num.mean())\n",
    "\n",
    "X_train_cat = X_train_cat.fillna(\"Missing\")\n",
    "X_test_cat = X_test_cat.fillna(\"Missing\")\n",
    "\n",
    "scaler = StandardScaler()\n",
    "X_train_scaled = pd.DataFrame(\n",
    "    scaler.fit_transform(X_train_num),\n",
    "    columns=continuous_features,\n",
    "    index=X_train.index\n",
    ")\n",
    "X_test_scaled = pd.DataFrame(\n",
    "    scaler.transform(X_test_num),\n",
    "    columns=continuous_features,\n",
    "    index=X_test.index\n",
    ")\n",
    "\n",
    "encoder = OneHotEncoder(handle_unknown=\"ignore\", sparse_output=False)\n",
    "encoder.fit(X_train_cat)\n",
    "\n",
    "X_train_encoded = pd.DataFrame(\n",
    "    encoder.transform(X_train_cat),\n",
    "    columns=encoder.get_feature_names_out(categorical_features),\n",
    "    index=X_train.index\n",
    ")\n",
    "X_test_encoded = pd.DataFrame(\n",
    "    encoder.transform(X_test_cat),\n",
    "    columns=encoder.get_feature_names_out(categorical_features),\n",
    "    index=X_test.index\n",
    ")\n",
    "\n",
    "X_train_final = pd.concat([X_train_scaled, X_train_encoded], axis=1)\n",
    "X_test_final = pd.concat([X_test_scaled, X_test_encoded], axis=1)\n",
    "\n",
    "display(X_train_final.head())\n"
   ]
